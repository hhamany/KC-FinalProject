<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/infooooo"
    tools:context=".PersonalInformation">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="-1dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="0dp">

        <com.airbnb.lottie.LottieAnimationView
            android:id="@+id/lottie3"
            android:layout_width="276dp"
            android:layout_height="264dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.503"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            app:lottie_autoPlay="true"
            app:lottie_loop="true"
            app:lottie_rawRes="@raw/docccccc"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/personalname"
            android:layout_width="268dp"
            android:layout_height="37dp"
            android:fontFamily="@font/fontyy"
            android:paddingRight="20dp"
            android:text="name"
            android:textColor="@color/white"
            android:textSize="20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.479"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/lottie3"
            app:layout_constraintVertical_bias="0.418" />

        <TextView
            android:id="@+id/personalage"
            android:layout_width="268dp"
            android:layout_height="37dp"
            android:fontFamily="@font/fontyy"
            android:paddingRight="20dp"
            android:text="age"
            android:textColor="@color/white"
            android:textSize="20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.479"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/lottie3" />

        <TextView
            android:id="@+id/personalphone"
            android:layout_width="268dp"
            android:layout_height="37dp"
            android:fontFamily="@font/fontyy"
            android:paddingRight="20dp"
            android:text="phone"
            android:textColor="@color/white"
            android:textSize="20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.479"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/lottie3"
            app:layout_constraintVertical_bias="0.583" />

        <TextView
            android:id="@+id/personalemail"
            android:layout_width="268dp"
            android:layout_height="37dp"
            android:fontFamily="@font/fontyy"
            android:paddingRight="20dp"
            android:text="email"
            android:textColor="@color/white"
            android:textSize="20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.479"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/lottie3"
            app:layout_constraintVertical_bias="0.671" />

        <TextView
            android:id="@+id/personalpass"
            android:layout_width="268dp"
            android:layout_height="37dp"
            android:fontFamily="@font/fontyy"
            android:paddingRight="20dp"
            android:text="pass"
            android:textColor="@color/white"
            android:textSize="20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.479"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/lottie3"
            app:layout_constraintVertical_bias="0.76" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</RelativeLayout>